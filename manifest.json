{
  "manifest_version": 3,
  "name": "eBird Fallout",
  "version": "0.5",
  "description": "eBird is already pretty great. But a few pages have data that I need to download. This extension lets you do that.",
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "*://*.ebird.org/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "ebirdfallout@sheerfocus.com",
      "strict_min_version": "121.0"
    }
  }
}